{
    "name": "geekfil/omnipay-payeer",
    "description": "Omnipay gateway for payeer.ocm",
    "keywords": [
        "omnipay",
        "payeer"
    ],
    "homepage": "https://github.com/geekfil/omnipay-payeer",
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Filin",
            "email": "geekfil@gmail.com",
            "homepage": "https://t.me/geekfil"
        }
    ],
    "require": {
        "omnipay/common": "v3.0-alpha.2"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "squizlabs/php_codesniffer": "^3"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Payeer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Payeer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
